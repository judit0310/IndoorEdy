<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns="http://dozer.sourceforge.net"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://dozer.sourceforge.net
          http://dozer.sourceforge.net/schema/beanmapping.xsd">

    <configuration>
        <stop-on-errors>true</stop-on-errors>
        <date-format>MM/dd/yyyy HH:mm:ss</date-format>
        <wildcard>true</wildcard>
    </configuration>

    <mapping>
        <class-a>net.opengis.gml.v_3_2_1.AbstractGMLType</class-a>
        <class-b>net.opengis.indoorgml.core.AbstractFeature</class-b>
        <field>
            <a>id</a>
            <b>gmlID</b>
        </field>
    </mapping>

    <mapping>
        <class-a>net.opengis.gml.v_3_2_1.AbstractFeatureType</class-a>
        <class-b>net.opengis.indoorgml.core.AbstractFeature</class-b>
    </mapping>

    <mapping>
        <class-a>net.opengis.indoorgml.core.v_1_0.IndoorFeaturesType</class-a>
        <class-b>net.opengis.indoorgml.core.IndoorFeatures</class-b>
        <field>
            <a set-method="setPrimalSpaceFeatures" get-method="getPrimalSpaceFeatures">PrimalSpaceFeaturesPropertyType</a>
            <b>PrimalSpaceFeatures</b>
        </field>

        <field>
            <a set-method="setMultiLayeredGraph" get-method="getMultiLayeredGraph">MultiLayeredGraphPropertyType</a>
            <b>MultiLayeredGraph</b>
        </field>
    </mapping>

    <mapping>
        <class-a>net.opengis.indoorgml.core.v_1_0.PrimalSpaceFeaturesPropertyType</class-a>
        <class-b>net.opengis.indoorgml.core.PrimalSpaceFeatures</class-b>
    </mapping>

    <mapping>
        <class-a>net.opengis.indoorgml.core.v_1_0.MultiLayeredGraphType</class-a>
        <class-b>net.opengis.indoorgml.core.MultiLayeredGraph</class-b>
    </mapping>

    <mapping>
        <class-a>net.opengis.indoorgml.core.v_1_0.SpaceLayerType</class-a>
        <class-b> net.opengis.indoorgml.core.SpaceLayer</class-b>
    </mapping>

    <mapping>
        <class-a>net.opengis.indoorgml.core.v_1_0.SpaceLayersType</class-a>
        <class-b> net.opengis.indoorgml.core.SpaceLayers</class-b>
    </mapping>

    <mapping>
        <class-a>net.opengis.indoorgml.core.v_1_0.CellSpaceType</class-a>
        <class-b>net.opengis.indoorgml.core.CellSpace</class-b>
    </mapping>

    <mapping>
        <class-a>net.opengis.indoorgml.core.v_1_0.CellSpaceBoundaryType</class-a>
        <class-b>net.opengis.indoorgml.core.CellSpaceBoundary</class-b>
    </mapping>

    <mapping>
        <class-a>net.opengis.gml.v_3_2_1.PointType</class-a>
        <class-b>net.opengis.indoorgml.geometry.Point</class-b>
    </mapping>

    <mapping>
        <class-a>net.opengis.indoorgml.core.Nodes</class-a>
        <class-b>net.opengis.indoorgml.core.v_1_0.NodesType</class-b>
        <field>
            <a set-method="setStateOnFloors" get-method="getStateOnFloors">stateOnFloor</a>
            <b>stateMember</b>
        </field>
    </mapping>

    <mapping type="bi-directional">
        <class-a>net.opengis.indoorgml.core.v_1_0.PrimalSpaceFeaturesType</class-a>
        <class-b>net.opengis.indoorgml.core.CellSpaceOnFloor</class-b>

        <field>
            <a set-method="setCellSpaceMember" get-method="getCellSpaceMember">cellSpaceMember</a>
            <b set-method="setCellSpaceMember" get-method="getCellSpaceMember">stateMember</b>
        </field>

    </mapping>
</mappings>